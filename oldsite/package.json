        "lodash": "^4.17.21",
        "motion": "^12.23.22",
        "next": "^15.5.4",
        "parse-numeric-range": "^1.3.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-error-boundary": "^6.0.0",
        "react-google-charts": "^5.2.1",
        "react-hooks-global-state": "^2.1.0",
        "react-markdown": "^10.1.0",
