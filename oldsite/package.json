{
    "name": "patik.com",
    "version": "1.0.0",
    "description": "Craig Patikâ€™s personal web site",
    "homepage": "https://patik.com",
    "private": true,
    "engines": {
        "node": "22"
    },
    "scripts": {
        "copyimages": "node ./bin/copy-images.mjs",
        "prebuild": "pnpm run copyimages",
        "clear-fetch-cache": "rm -rf tmp",
        "build": "pnpm clear-fetch-cache; pnpm run copyimages && next build",
        "start": "pnpm run copyimages && next dev",
        "start:prod": "pnpm run clear-fetch-cache; next build && npx serve@latest out",
        "lint": "pnpm next lint && pnpm tsc --noEmit && pnpm find-unused-sass-variables src/styles",
        "test": "pnpm run test:blog",
        "test:blog": "jest --config ./jest.config.js",
        "clean": "rm -rf node_modules; rm -rf out; rm -rf .next; rm next-env.d.ts; pnpm run clear-fetch-cache"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "classnames": "^2.5.1",
        "cloudinary": "^2.7.0",
